<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" fitToHeight="true" fitToWidth="true"
            xmlns="http://javafx.com/javafx/8.0.121" fx:controller="ru.spbau.mit.oquechy.pair.Controller">
    <VBox alignment="TOP_CENTER" minHeight="600.0" minWidth="600.0" prefHeight="600.0" prefWidth="600.0"
          spacing="12.0">
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
        </padding>
        <HBox alignment="CENTER" minHeight="40.0" minWidth="100.0" spacing="20.0">
            <Button mnemonicParsing="false" onMouseClicked="#setUpField" text="New game"
                    textFill="#5b4545"/>
        </HBox>
        <GridPane fx:id="grid" alignment="CENTER" hgap="10" maxHeight="1.7976931348623157E308"
                  maxWidth="1.7976931348623157E308" vgap="10" VBox.vgrow="ALWAYS">
        </GridPane>
    </VBox>
</ScrollPane>
